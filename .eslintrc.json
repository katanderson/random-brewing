{
  "extends": "eslint-config-react-yas",
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "semi": true,
        "printWidth": 100,
        "arrowParens": "always",
        "bracketSameLine": false,
        "jsxSingleQuote": false
      }
    ]
  }
}
